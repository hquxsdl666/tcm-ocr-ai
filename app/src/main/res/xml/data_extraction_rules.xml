<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- 备份共享偏好设置 -->
        <include domain="sharedpref" path="."/>
        <!-- 排除包含敏感信息的文件 -->
        <exclude domain="sharedpref" path="tcm_prefs.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- 备份共享偏好设置 -->
        <include domain="sharedpref" path="."/>
        <!-- 排除包含敏感信息的文件 -->
        <exclude domain="sharedpref" path="tcm_prefs.xml"/>
    </device-transfer>
</data-extraction-rules>
